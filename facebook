int z, trial =0;
double h= 0;
double t= 0;
while (trial < 100000){
   z=randint(0,1);
   if(z==0){
   h= h+1;
}else{
   if(z==1);
   t =t+1;
}
trial++;
}
 
printf("Coin face:           Frequency:         probability:\n");
printf("   Heads                %.2lf           %.2lf%%          \n",h, h*100/trial);
printf("   Tails                %.2lf           %.2lf%%          \n",t, t*100/trial);

